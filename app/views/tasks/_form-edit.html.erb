<h1>Edit Task</h1>
<%= form_with model: task do |f| %>
  <div>
   <%= f.label :title %>
   <%= f.text_field :title %>
  </div>

  <div>
   <%= f.label :details %>
   <%= f.text_field :details %>
  </div>

  <div>
    <%= f.submit 'Edit Task' %>
  </div>
<% end %>
<%= link_to 'Back to Tasks', tasks_path %>
